{
  "name": "snapwright",
  "displayName": "SnapWright",
  "description": "Playwright TypeScript productivity booster - Generate PageFactory patterns and Page Object Models with ease",
  "version": "1.3.0",
  "publisher": "altajvirani",
  "repository": {
    "type": "git",
    "url": "https://github.com/altajvirani/snapwright.git"
  },
  "homepage": "https://github.com/altajvirani/snapwright#readme",
  "bugs": {
    "url": "https://github.com/altajvirani/snapwright/issues"
  },
  "keywords": [
    "playwright",
    "typescript",
    "page-object-model",
    "test-automation",
    "e2e-testing",
    "pom",
    "pageFactory",
    "productivity",
    "code-generator",
    "testing-tools"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets",
    "Testing",
    "Programming Languages",
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snapwright.createPageFactory",
        "title": "Create Playwright PageFactory",
        "category": "SnapWright"
      },
      {
        "command": "snapwright.addPOMToFactory",
        "title": "Add Page Objects to Factory",
        "category": "SnapWright"
      },
      {
        "command": "snapwright.createPOMClass",
        "title": "Create Page Object Class",
        "category": "SnapWright"
      },
      {
        "command": "snapwright.usePOMFromFactory",
        "title": "Use POM from PageFactory",
        "category": "SnapWright"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/pom-snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "author": {
    "name": "Altaj Virani",
    "email": "altajvirani@example.com"
  },
  "license": "MIT",
  "icon": "SnapWright_Logo.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  }
}
