{
  "name": "snapwright",
  "displayName": "SnapWright POM Generator",
  "description": "Generate Page Object Model classes and PageFactory patterns for Playwright test automation with smart duplicate detection and code snippets",
  "version": "1.0.0",
  "publisher": "altajvirani",
  "repository": {
    "type": "git",
    "url": "https://github.com/altajvirani/snapwright.git"
  },
  "homepage": "https://github.com/altajvirani/snapwright#readme",
  "bugs": {
    "url": "https://github.com/altajvirani/snapwright/issues"
  },
  "keywords": [
    "playwright",
    "page-object-model",
    "test-automation",
    "pom",
    "pageFactory",
    "code-generator",
    "testing",
    "typescript"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onCommand:snapwright.createPageFactory",
    "onCommand:snapwright.addPOMToFactory",
    "onCommand:snapwright.createPageFactoryInstance"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "snapwright.createPageFactory",
        "title": "Create PageFactory",
        "category": "SnapWright"
      },
      {
        "command": "snapwright.addPOMToFactory",
        "title": "Add POM Classes to PageFactory",
        "category": "SnapWright"
      },
      {
        "command": "snapwright.createPageFactoryInstance",
        "title": "Create PageFactory Instance (Smart Import)",
        "category": "SnapWright"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/pom-snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "author": {
    "name": "Altaj Virani",
    "email": "altajvirani@example.com"
  },
  "license": "MIT",
  "icon": "SnapWright_Logo.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  }
}
